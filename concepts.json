          "example": ["(|>) : a -> (a -> b) -> b",
											"",
											"employeeBossStreet : Employee -> String",
											"employeeBossStreet employee =   employee",
											"    |> boss",
											"    |> address",
											"    |> street",
											"",
											"employeeBossStreet dummyEmployeeRecord",
											">\"DummyEmployeeBossStreetName\" : String."
										]
